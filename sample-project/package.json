{
  "name": "sample-project",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.8",
    "@hotwired/stimulus": "^3.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.2",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "stimulus": "^3.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-assets-manifest": "5.0.6",
    "webpack-cli": "^4.9.2",
    "yarn-check": "^0.0.3"
  },
  "jest": {
    "testRegex": ".*_spec.js",
    "roots": [
      "<rootDir>/spec/javascript/jest"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/app/javascript"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  }
}
